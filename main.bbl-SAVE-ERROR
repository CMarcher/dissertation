      \strng{namehash}{e7f6ebdcc2af8d00ca0afe2ff8a608e1}
      \strng{fullhash}{e7f6ebdcc2af8d00ca0afe2ff8a608e1}
      \strng{fullhashraw}{e7f6ebdcc2af8d00ca0afe2ff8a608e1}
